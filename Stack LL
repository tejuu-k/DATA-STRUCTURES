#include <stdio.h>
#include <stdlib.h>

struct node {
    int data;
    struct node *link;
};

struct node *top = NULL;

void push(int data) {
    struct node* newNode = malloc(sizeof(struct node));
    newNode->data = data;
    newNode->link = top;
    top = newNode;
}

int pop() {
    if (top == NULL) {
        printf("Stack Underflow\n");
        return -1;
    }

    struct node* temp = top;
    int data = temp->data;
    top = top->link;
    free(temp);
    return data;
}

void display() {
    struct node* temp = top;
    while (temp != NULL) {
        printf("%d -> ", temp->data);
        temp = temp->link;
    }
    printf("NULL\n");
}

int main() {
    push(10);
    push(20);
    push(30);
    push(40);
    push(50);

    display();

    printf("Popped: %d\n", pop());
    display();

    printf("Popped: %d\n", pop());
    display();

    return 0;
}
OUTPUT:
50 -> 40 -> 30 -> 20 -> 10 -> NULL
Popped: 50
40 -> 30 -> 20 -> 10 -> NULL
Popped: 40
30 -> 20 -> 10 -> NULL
