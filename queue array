#include <stdio.h>
#include <stdlib.h>

struct node {
    int data;
    struct node *link;
};

struct node *front = NULL;
struct node *rear = NULL;

void enqueue(int data) {
    struct node* newNode = malloc(sizeof(struct node));
    newNode->data = data;
    newNode->link = NULL;

    if (rear == NULL) { // Queue empty
        front = rear = newNode;
    } else {
        rear->link = newNode;
        rear = newNode;
    }
}

int dequeue() {
    if (front == NULL) {
        printf("Queue Underflow\n");
        
    }

    struct node* temp = front;
    int data = temp->data;

    front = front->link;
    if (front == NULL)
        rear = NULL;

    free(temp);
    return data;
}

void display() {
    struct node* temp1 = front;
    while (temp1 != NULL) {
        printf("%d -> ", temp1->data);
        temp1 = temp1->link;
    }
    printf("NULL\n");
}

int main() {
    enqueue(10);
    enqueue(20);
    enqueue(30);
    enqueue(40);
    enqueue(50);

    display();

    printf("Dequeued: %d\n", dequeue());
   display();

    return 0;
}
