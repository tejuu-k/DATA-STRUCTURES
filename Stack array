#include <stdio.h>
#include <stdlib.h>
#define MAX 5

int stack[MAX];
int top = -1;

void push(int value) {
    if (top == MAX - 1) {
        printf("Overflow: Stack is full\n");
    } else {
        top++;
        stack[top] = value;
        printf("%d pushed\n", value);
    }
}

int pop() {
    if (top == -1) {
        printf("Underflow: Stack is empty\n");
        return -1;
    } else {
        int data = stack[top];
        top--;
        return data;
    }
}

int peek() {
    if (top == -1) {
        printf("Stack is empty, cannot peek\n");
        return -1;
    } else {
        return stack[top];
    }
}

void display() {
    if (top == -1) {
        printf("Stack is empty\n");
    } else {
        printf("Stack elements: ");
        for (int i = 0; i <= top; i++) {
            printf("%d ", stack[i]);
        }
        printf("\n");
    }
}

void main() {
    push(10);
    push(20);
    push(30);
    push(40);
    push(50);
    push(60);

    display();

    printf("Popped: %d\n", pop());
    printf("Popped: %d\n", pop());

    display();

    printf("Peek element: %d\n", peek());
}
OUTPUT:
10 pushed
20 pushed
30 pushed
40 pushed
50 pushed
Overflow: Stack is full
Stack elements: 10 20 30 40 50 
Popped: 50
Popped: 40
Stack elements: 10 20 30 
Peek element: 30
